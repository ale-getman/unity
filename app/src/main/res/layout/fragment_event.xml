        android:layout_height="180dp"
        app:srcCompat="@mipmap/ic_launcher"
        tools:layout_editor_absoluteX="-608dp"
        tools:layout_editor_absoluteY="311dp" />

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/profile_image"
            android:layout_width="99dp"
            android:layout_height="97dp"
            android:layout_marginEnd="72dp"
            android:layout_marginStart="72dp"
            android:src="@mipmap/ic_launcher"
            app:civ_border_color="#cccccc"
            app:civ_border_width="2dp"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/id_navbar_count"
            app:layout_constraintRight_toRightOf="@+id/text_coun_rate"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/textView3"
            app:layout_constraintTop_toTopOf="@+id/id_navbar_count" />

        <TextView
            android:id="@+id/id_navbar_count"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="880"
            android:textColor="@android:color/holo_orange_dark"
            android:textStyle="bold"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/text_coun_rate"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="7dp"
            app:layout_constraintLeft_toLeftOf="@+id/text_coun_rate" />

        <TextView
            android:id="@+id/text_coun_rate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/text_ballow"
            android:textColor="@android:color/darker_gray"
            android:textStyle="bold"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            android:layout_marginEnd="83dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="233dp"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/id_persanal_hi"
            android:layout_width="236dp"
            android:layout_height="60dp"
            android:gravity="center_vertical|center_horizontal"
            android:text="Добрый день, Klippe!"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/profile_image"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="20dp"
            app:layout_constraintLeft_toLeftOf="parent" />
