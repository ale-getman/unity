<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="@color/btn_white_red" />
            <padding
                android:left="20dp"
                android:top="1dp"
                android:right="20dp"
                android:bottom="1dp" />
            <corners android:radius="20dp"/>
            <size android:height="50dp" android:width="50dp"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid
                android:color="#a0a7ac"/>
            <padding
                android:left="20dp"
                android:top="1dp"
                android:right="20dp"
                android:bottom="1dp" />
            <corners android:radius="20dp"/>
            <size android:height="50dp" android:width="50dp"/>
        </shape>
    </item>
    <!-- 32aeea 919296 06189F -->
    <item>
        <!--<bitmap android:src="@drawable/ic_refresh"/>-->
        <shape>
            <solid
                android:color="@color/btn_red" />
            <padding
                android:left="20dp"
                android:top="1dp"
                android:right="20dp"
                android:bottom="1dp" />
            <corners android:radius="20dp"/>
            <size android:height="50dp" android:width="50dp"/>
        </shape>
    </item>
</selector>